<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.afpa.controllers.Table">
   <children>
      <Label layoutX="14.0" layoutY="14.0" text="Table Exercise" textFill="#791066">
         <font>
            <Font size="28.0" />
         </font>
      </Label>
      <TableView fx:id="clientList" layoutX="14.0" layoutY="55.0" prefHeight="332.0" prefWidth="408.0">
        <columns>
          <TableColumn fx:id="firstName" prefWidth="140.0" text="Prénom" />
          <TableColumn fx:id="lastName" prefWidth="140.0" text="Nom" />
            <TableColumn fx:id="city" prefWidth="130.0" text="Ville" />
        </columns>
      </TableView>
      <Label layoutX="438.0" layoutY="55.0" text="Prénom">
         <font>
            <Font size="23.0" />
         </font>
      </Label>
      <TextField fx:id="firstNameInput" layoutX="438.0" layoutY="92.0" prefHeight="26.0" prefWidth="154.0" promptText="Prénom..." />
      <Label layoutX="438.0" layoutY="136.0" text="Nom">
         <font>
            <Font size="23.0" />
         </font>
      </Label>
      <TextField fx:id="lastNameInput" layoutX="438.0" layoutY="174.0" prefHeight="26.0" prefWidth="154.0" promptText="Nom..." />
      <Label layoutX="438.0" layoutY="221.0" text="Ville">
         <font>
            <Font size="23.0" />
         </font>
      </Label>
      <TextField fx:id="cityInput" layoutX="438.0" layoutY="257.0" prefHeight="26.0" prefWidth="154.0" promptText="Ville..." />
      <Button layoutX="434.0" layoutY="305.0" mnemonicParsing="false" onAction="#save" text="Sauver" />
      <Button layoutX="515.0" layoutY="305.0" mnemonicParsing="false" onAction="#reset" text="Annuler" />
      <Button layoutX="432.0" layoutY="345.0" mnemonicParsing="false" onAction="#delete" prefHeight="26.0" prefWidth="154.0" text="Supprimer" />
   </children>
</AnchorPane>
